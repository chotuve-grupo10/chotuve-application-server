[FORMAT]
indent-string=\t

[MASTER]
load-plugins=pylintplugins
disable=
    C0111, # missing docstring
    fixme,
    logging-not-lazy,
    wildcard-import,
    unused-wildcard-import,
    bad-continuation,
    redefined-outer-name,
    line-too-long,
    too-many-statements,
    R0801 # me hubiera encantado agregarlo solo en los tests pero no lo logr√©,
    logging-format-interpolation
